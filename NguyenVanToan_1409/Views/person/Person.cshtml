<h2>Danh sach nguoi</h2>
<hr>
<table class="table boder">
    <tr>
        <th>
            <label>ID</label>
        </th>

        <th>
            <label>Ten </label>
        </th>

        <th>
            <label>Tuoi</label>
        </th>
    </tr>
    @foreach (var item in ViewData["Persons"] as List<Person>)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Age</td>
        </tr>
    }
</table>
